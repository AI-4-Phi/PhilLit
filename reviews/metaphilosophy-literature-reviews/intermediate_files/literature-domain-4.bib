@article{graness2018questions,
  author = {Graness, Anke},
  title = {Questions of Canon Formation in Philosophy: The History of Philosophy in Africa},
  journal = {Phronimon},
  year = {2018},
  volume = {16},
  number = {2},
  pages = {78--96},
  doi = {10.25159/2413-3086/3819},
  note = {
  CORE ARGUMENT: Graness examines the challenges of writing a history of philosophy
  for African traditions, arguing that the discipline of history of philosophy has
  been instrumental in shaping exclusionary canons. She shows how African philosophical
  thought (traceable to ancient Egypt) has been systematically excluded through
  historiographical practices that privilege certain textual forms and conceptual
  frameworks. Graness argues that decolonizing the history of philosophy requires
  "conceptual decolonisation" (following Wiredu)—questioning the very categories
  through which philosophy is defined. The difficulty of writing African philosophy's
  history reveals how Eurocentric frameworks structure the discipline itself.

  RELEVANCE: Demonstrates how historiographical practices shape canons and what
  counts as "philosophy." Directly relevant to literature review methodology because
  it shows how definitional questions precede empirical surveys—deciding what
  counts as "philosophical literature" already enacts exclusions. Graness's emphasis
  on conceptual decolonization suggests literature reviews must interrogate their
  own categorical frameworks, not just expand inclusion within existing categories.

  POSITION: Decolonial critique of history of philosophy as canon-forming practice.
  Emphasizes conceptual and categorical dimensions of exclusion.
  },
  keywords = {canon-formation, decolonial, African-philosophy, historiography, High},
  abstract = {The history of philosophy is not just an academic discipline, but considered to be a philosophical activity itself. It has been instrumental in shaping our understanding of philosophy, our philosophical canon and curricula. The history of philosophy in Africa is still a young discipline, although philosophical thinking (concepts, manuscripts, books and philosophers) can be traced back until ancient Egypt. Facing the problem of exclusion and inferiorisation of traditions of thought and philosophy in Africa, the discipline of the history of philosophy involves very specific problems and requires a project of “conceptual decolonisation”. This explains both the importance and the difficulty of writing a history of philosophy in Africa.},
  abstract_source = {s2},
}

