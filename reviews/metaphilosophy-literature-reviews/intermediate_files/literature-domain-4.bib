@article{dhawan2017noneuropeans,
  author = {Dhawan, Nikita},
  title = {Can Non‐Europeans Philosophize? Transnational Literacy and Planetary Ethics in a Global Age},
  journal = {Hypatia},
  year = {2017},
  volume = {32},
  number = {3},
  pages = {488--505},
  doi = {10.1111/hypa.12333},
  note = {
  CORE ARGUMENT: Dhawan critiques the Eurocentric presumption that philosophy is
  fundamentally European, showing how Enlightenment universalism was constructed
  through colonial exclusions. She debunks claims that Kantian cosmopolitanism
  provided an antidote to colonialism, instead documenting how Enlightenment
  philosophy participated in epistemic violence that delegitimized non-European
  thought. Dhawan argues that simply "including" previously marginalized traditions
  within existing philosophical frameworks proves insufficient—the frameworks
  themselves must be decolonized. Drawing on Spivak, she proposes "transnational
  literacy" and "planetary ethics" as alternatives to liberal multicultural inclusion.

  RELEVANCE: Addresses the fundamental question of what counts as "philosophy" and
  whose knowledge receives epistemic recognition. Directly relevant to literature
  reviews because it questions whether adding "diverse voices" to existing review
  structures addresses the deeper epistemic frameworks that enabled exclusion.
  Dhawan's critique of liberal inclusion suggests literature reviews require
  methodological transformation, not just expanded scope. Her emphasis on uneven
  epistemic agency illuminates power dynamics in citation and recognition practices.

  POSITION: Postcolonial feminist critique of Enlightenment universalism and
  Eurocentric philosophy. Argues for epistemic decolonization beyond inclusion.
  },
  keywords = {decolonial, Eurocentrism, Enlightenment, postcolonial-feminism, High},
  abstract = {Defenders of the Enlightenment highlight the long neglected anticolonial writings of thinkers like Immanuel Kant, which serve as a corrective to the misrepresentation of the Enlightenment's epistemological investment in imperialism. One of the most pervasive repercussions of the claim that the Enlightenment was always already anti‐imperial is that postcolonial critique is rendered redundant, and the project of decolonizing European philosophy becomes unnecessary. Contesting the exoneration of Enlightenment philosophers of racism and sexism, this article debunks the claim that Kantian cosmopolitanism was an antidote to colonialism. Addressing the ambivalent legacies of the European Enlightenment for the postcolonial world, with special focus on the “Syrian refugee crisis,” the article examines the enduring normative violence exerted by Enlightenment principles of cosmopolitanism and outlines the contested terrains that inflect current geopolitics of knowledge‐production. Given that the normative idea of philosophy, as defined during the Enlightenment, continues to delegitimize non‐European perspectives, the integration of previously marginalized knowledges into the philosophical canon is insufficient; rather, in order to desubalternize non‐Western epistemologies, it is imperative to undo the uneven distribution of epistemic agency globally. Drawing on Gayatri Spivak's ideas of transnational literacy and planetary ethics, the article concludes by underscoring the contribution of postcolonial‐feminist critique in imagining postimperial philosophy in a global age.},
  abstract_source = {openalex},
}

@article{grosfoguel2013structure,
  author = {Grosfoguel, Ramón},
  title = {The Structure of Knowledge in Westernized Universities: Epistemic Racism/Sexism and the Four Genocides/Epistemicides of the Long 16th Century},
  journal = {Human Architecture: Journal of the Sociology of Self-Knowledge},
  year = {2013},
  volume = {11},
  number = {1},
  pages = {73--90},
  note = {
  CORE ARGUMENT: Grosfoguel argues that Westernized universities are structured by
  "epistemic racism/sexism"—the systematic privileging of knowledge produced by men
  from five Western European countries as universally valid while dismissing all other
  knowledge as particular, traditional, or non-scientific. He traces this structure
  to four epistemicides of the long 16th century: conquest of Al-Andalus, conquest
  of Americas, African enslavement, and European witch hunts. These genocides
  destroyed entire knowledge systems and ways of knowing. Modern disciplines embody
  this epistemic structure, with theory reduced to white European male experience
  presented as universal.

  RELEVANCE: Provides theoretical framework for understanding how canon formation
  operates as "epistemicide"—not just exclusion but active destruction of knowledge
  systems. Directly relevant to literature reviews because it challenges the assumption
  that "relevant literature" can be identified through neutral bibliographic search—
  the very infrastructure of knowledge production embeds exclusionary structures.
  Grosfoguel's emphasis on epistemicide suggests literature reviews participate in
  reproducing or contesting colonial knowledge structures.

  POSITION: Decolonial critique theorizing epistemic violence and epistemicide.
  Connects historical colonial violence to contemporary university structures.
  },
  keywords = {decolonial, epistemicide, epistemic-violence, Eurocentrism, High, INCOMPLETE, no-abstract}
}

@article{kim2019alterity,
  author = {Kim, David Haekwon},
  title = {Alterity, Analectics, and the Challenges of Epistemic Decolonization},
  journal = {The Southern Journal of Philosophy},
  year = {2019},
  volume = {57},
  number = {S1},
  pages = {43--70},
  doi = {10.1111/sjp.12346},
  note = {
  CORE ARGUMENT: Kim distinguishes between "alterior" philosophies (culturally
  different from Western traditions) and "analectical" philosophies (emerging from
  politically subordinated positions, following Dussel). He examines challenges of
  epistemic decolonization in US philosophy, arguing that simply including
