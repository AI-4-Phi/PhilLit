@article{berges2020outskirts,
  author = {Bergès, Sandrine},
  title = {On the Outskirts of the Canon: The Myth of the Lone Female Philosopher, and What to Do about It},
  journal = {Journal of the American Philosophical Association},
  year = {2020},
  volume = {6},
  number = {1},
  pages = {1--18},
  doi = {10.1017/apa.2019.36},
  note = {
  CORE ARGUMENT: Bergès examines why recovered women philosophers remain on "the
  outskirts of the canon"—known individually but not integrated into standard
  narratives of philosophy's development. She argues the "lone female philosopher"
  framing (pairing each woman with a canonical male figure) obscures the intellectual
  networks and conversations among women philosophers themselves. Bergès proposes
  three strategies: creating courses focused entirely on women philosophers to
  establish their intellectual traditions, pairing women with canonical men as
  interlocutors, and reconstructing women's intellectual networks. She examines
  barriers to integration including pedagogical structures and historiographical
  conventions.

  RELEVANCE: Addresses why recovery work alone doesn't transform canons, relevant
  to debates about literature review reform. Bergès's analysis of the "lone female
  philosopher" problem illuminates how inclusion can occur without challenging
  underlying narrative structures. Her pedagogical focus connects to how literature
  reviews teach readers what counts as "the conversation" in a field. The emphasis
  on intellectual networks rather than isolated figures suggests literature reviews
  could reconstruct marginalized traditions rather than just adding individual voices.

  POSITION: Feminist historiography examining barriers to canon integration beyond
  recovery. Proposes pedagogical and narrative strategies for transformation.
  },
  keywords = {feminist-philosophy, canon-formation, women-philosophers, pedagogy, Medium},
  abstract = {Abstract Philosophical views of responsibility often identify responsible agency with capacities such as rationality and self-control. Yet in ordinary life, we frequently hold individuals responsible who are deficient in these capacities, such as children or people with mental illness. The existing literature that addresses these cases has suggested that we merely pretend to hold these agents responsible or that they are responsible to a diminished degree. In this paper, I demonstrate that neither of these approaches is satisfactory, and I offer an alternative focused on the role relationships play in determining whether it is appropriate to hold someone responsible. I argue that relationships are sources of normative expectations about how parties in that relationship ought to behave and that we can be responsible in virtue of being subject to these norms. This is so not only for those who are impaired or immature, but for all of us.},
  abstract_source = {s2},
}

